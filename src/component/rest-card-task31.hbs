{{#with cardName}}
<div class="card text-center">
    <div class="card-header">
        {{header}}
    </div>  
    <div class="card-body">
        <div class="card-title">
            {{title}}
        </div>
        <p>{{description}}</p>
        <button class="btn btn-primary">{{button}}</button>
    </div>
    <div class="card-footer">
        {{footer}}
    </div>
</div>
{{/with}}